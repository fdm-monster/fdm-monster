{
  "name": "fdm-monster",
  "author": "David Zwart",
  "license": "AGPL-3.0-or-later",
  "version": "1.4.5",
  "description": "FDM Monster is a bulk OctoPrint manager to set up, configure and monitor 3D printers. Our aim is to provide extremely optimized websocket performance and reliability.",
  "main": "index.mjs",
  "scripts": {
    "prestart": "npm ci --production",
    "start": "pm2 flush && pm2 start index.mjs --name FDM -e ./logs/pm2.error.log -o ./logs/pm2.out.log --time --restart-delay=1000 --update-env --exp-backoff-restart-delay=1500",
    "restart": "pm2 restart FDM",
    "pupdate": "pm2 update",
    "stop": "pm2 stop FDM",
    "stop:delete": "pm2 delete FDM",
    "migration:create": "migrate-mongo create printer-remove-sortindex",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "dev": "nodemon index.mjs",
    "test": "jest --forceExit",
    "test:cov": "jest --forceExit --coverage",
    "cov:show": "./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fdm-monster/fdm-monster"
  },
  "bin": {
    "fdm-monster": "./index.mjs"
  },
  "keywords": [
    "fdm-monster",
    "fdm-connector",
    "octoprint",
    "fdm",
    "3d printing",
    "pi",
    "node",
    "vue"
  ],
  "dependencies": {
    "@fdm-monster/client": "1.3.3",
    "@influxdata/influxdb-client": "1.33.2",
    "@mikro-orm/better-sqlite": "5.7.14",
    "@mikro-orm/core": "5.7.14",
    "@sentry/node": "7.67.0",
    "adm-zip": "0.5.10",
    "awilix": "8.0.1",
    "awilix-express": "8.0.0",
    "axios": "1.5.0",
    "bcryptjs": "2.4.3",
    "cache-manager": "4.1.0",
    "connect-history-api-fallback": "2.0.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "eventemitter2": "6.4.9",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "form-data": "4.0.0",
    "got": "11.8.6",
    "helmet": "7.0.0",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "luxon": "3.4.3",
    "migrate-mongo": "10.0.0",
    "mongoose": "6.12.0",
    "mqtt": "5.0.4",
    "multer": "1.4.5-lts.1",
    "node-input-validator": "4.5.1",
    "nodemon": "3.0.1",
    "octokit": "3.1.0",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "semver": "7.5.4",
    "simple-git": "3.19.1",
    "socket.io": "4.7.2",
    "toad-scheduler": "3.0.0",
    "uuid": "9.0.0",
    "winston": "3.10.0",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@lcov-viewer/cli": "1.3.0",
    "@lcov-viewer/istanbul-report": "1.4.0",
    "@mikro-orm/cli": "5.7.14",
    "@mikro-orm/migrations": "5.7.14",
    "caniuse-lite": "1.0.30001527",
    "eslint": "8.48.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.6.4",
    "jest-27-expect-message": "1.1.0",
    "jest-util": "29.6.3",
    "mongodb-memory-server": "8.15.1",
    "nock": "13.3.3",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "supertest": "6.3.3"
  },
  "resolutions": {
    "got": "11.8.6"
  },
  "ignore-engines": false,
  "engine-strict": true,
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 8.1.4",
    "yarn": ">= 1.22.10"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      ".github/*",
      ".docs/*",
      "./coverage/*",
      "file-uploads",
      "logs/*",
      "test/*",
      "images/*",
      "views/*",
      "docker/*",
      "client-vue/*"
    ],
    "delay": 2000
  }
}
