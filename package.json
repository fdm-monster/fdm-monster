{
  "name": "@fdm-monster/server",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdm-monster/fdm-monster.git"
  },
  "author": "David Zwart",
  "license": "AGPL-3.0-or-later",
  "version": "2.0.10",
  "bin": {
    "fdm-monster-server": "dist/index.js",
    "fdmm-server": "dist/index.js"
  },
  "description": "FDM Monster is a bulk OctoPrint, Klipper, PrusaLink and BambuLab manager to set up, configure and monitor 3D printers. Our aim is to provide neat overview over your farm.",
  "main": "dist/index.js",
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths",
    "dev": "cross-env NODE_ENV=development node --watch --enable-source-maps dist/index.js",
    "dev:build": "swc src -w -d dist --strip-leading-paths",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "console:http-receivers": "node dist/consoles/launch-http-servers.js",
    "console:github-releases": "cross-env NODE_ENV=development node --watch --enable-source-maps dist/consoles/download-github-releases.js",
    "console:create": "tsx src/consoles/typeorm-create.ts",
    "console:generate": "tsx src/consoles/typeorm-generate.ts",
    "console:migrate": "tsx src/consoles/typeorm-migrate.ts",
    "console:mock-octoprint": "cross-env NODE_ENV=development node --enable-source-maps dist/consoles/mock-octoprint.server.js",
    "console:mock-bambu": "cross-env NODE_ENV=development node --enable-source-maps dist/consoles/mock-bambu.server.js",
    "console:bambu-diagnostic": "cross-env NODE_ENV=development node --enable-source-maps dist/consoles/bambu-mqtt-diagnostic.console.js",
    "console:mock-camera": "cross-env NODE_ENV=development node --enable-source-maps dist/consoles/mock-camera/mock-camera.server.js",
    "format": "biome format --write src test",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "torm": "tsx --tsconfig tsconfig.json ./node_modules/typeorm/cli.js",
    "typeorm:create": "yarn torm migration:create src/migrations/DropPrintCompletion",
    "typeorm:generate": "yarn torm migration:generate -d src/data-source.ts --pretty src/migrations/AddPrintJob",
    "typeorm:migrate": "yarn torm migration:run -d src/data-source.ts",
    "typeorm:revert": "yarn torm migration:revert -d src/data-source.ts"
  },
  "keywords": [
    "fdm-monster",
    "octoprint",
    "moonraker",
    "klipper",
    "bambu",
    "prusalink",
    "fdm",
    "3d printing",
    "pi",
    "node",
    "vue",
    "MonsterPi",
    "RaspberryPi",
    "Unraid"
  ],
  "resolutions": {
    "tr46": "6.0.0"
  },
  "dependencies": {
    "@fdm-monster/client-next": "2.3.3",
    "@octokit/plugin-throttling": "11.0.3",
    "@sentry/node": "10.38.0",
    "adm-zip": "0.5.16",
    "awilix": "12.1.1",
    "awilix-express": "11.0.1",
    "axios": "1.13.5",
    "basic-ftp": "5.1.0",
    "bcryptjs": "3.0.3",
    "better-sqlite3": "12.6.2",
    "class-validator": "0.14.3",
    "connect-history-api-fallback": "2.0.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.6",
    "cross-env": "10.1.0",
    "dotenv": "17.3.1",
    "eventemitter2": "6.4.9",
    "express": "4.22.1",
    "form-data": "4.0.5",
    "helmet": "8.1.0",
    "js-yaml": "4.1.1",
    "jsonwebtoken": "9.0.3",
    "lodash": "4.17.23",
    "lodash-es": "^4.17.23",
    "luxon": "3.7.2",
    "mqtt": "5.15.0",
    "multer": "2.0.2",
    "octokit": "5.0.5",
    "passport": "0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "prom-client": "15.1.3",
    "reflect-metadata": "0.2.2",
    "semver": "7.7.4",
    "socket.io": "4.8.3",
    "toad-scheduler": "3.1.0",
    "typeorm": "0.3.28",
    "uuid": "11.1.0",
    "winston": "3.19.0",
    "winston-loki": "6.1.4",
    "ws": "8.19.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@lcov-viewer/cli": "1.3.0",
    "@lcov-viewer/istanbul-report": "1.4.0",
    "@swc/cli": "0.8.0",
    "@swc/core": "1.15.11",
    "@types/adm-zip": "0.5.7",
    "@types/connect-history-api-fallback": "1.5.4",
    "@types/cookie-parser": "1.4.10",
    "@types/express": "5.0.1",
    "@types/ip": "1.1.3",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.17.23",
    "@types/lodash-es": "^4",
    "@types/luxon": "3.7.1",
    "@types/multer": "2.0.0",
    "@types/node": "24.10.13",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "4.0.1",
    "@types/semver": "7.7.1",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.18.1",
    "@vitest/ui": "4.0.18",
    "chokidar": "5.0.0",
    "ftp-srv": "4.6.3",
    "nock": "13.5.6",
    "openapi3-ts": "4.5.0",
    "selfsigned": "5.5.0",
    "supertest": "7.2.2",
    "swagger-ui-dist": "5.31.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.19.2",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  },
  "ignore-engines": false,
  "engine-strict": true,
  "engines": {
    "node": ">= 22.0.0",
    "npm": ">= 10.9.0",
    "yarn": ">= 4.12.0"
  },
  "packageManager": "yarn@4.12.0"
}
