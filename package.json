{
  "name": "@fdm-monster/server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdm-monster/fdm-monster.git"
  },
  "author": "David Zwart",
  "license": "AGPL-3.0-or-later",
  "version": "2.0.3",
  "bin": {
    "fdm-monster-server": "dist/index.js",
    "fdmm-server": "dist/index.js"
  },
  "description": "FDM Monster is a bulk OctoPrint, Klipper, PrusaLink and BambuLab manager to set up, configure and monitor 3D printers. Our aim is to provide neat overview over your farm.",
  "main": "dist/index.js",
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths",
    "build:watch": "swc src -w -d dist --strip-leading-paths",
    "dev": "yarn build:watch & yarn serve",
    "serve": "NODE_ENV=development node --enable-source-maps --watch-path=dist dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "console:many-http-file-receivers": "node ./dist/consoles/launch-http-servers.js",
    "console:github-releases": "swc clean && yarn build && NODE_ENV=development node --watch --enable-source-maps ./dist/consoles/download-github-releases.js",
    "console:create": "swc clean && yarn build && NODE_ENV=development node --enable-source-maps ./dist/consoles/typeorm-create.js",
    "console:generate": "swc clean && yarn build && NODE_ENV=development node --enable-source-maps ./dist/consoles/typeorm-generate.js",
    "console:migrate": "swc clean && yarn build && NODE_ENV=development node --watch --enable-source-maps ./dist/consoles/typeorm-migrate.js",
    "console:mock-octoprint": "swc clean && yarn build && NODE_ENV=development node --enable-source-maps ./dist/consoles/mock-octoprint.server.js",
    "console:mock-bambu": "swc clean && yarn build && NODE_ENV=development node --enable-source-maps ./dist/consoles/mock-bambu.server.js",
    "console:mock-camera": "swc clean && yarn build && NODE_ENV=development node --enable-source-maps ./dist/consoles/mock-camera/mock-camera.server.js",
    "format": "biome format --write src test",
    "swc": "swc",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "test": "jest --forceExit  --maxConcurrency=8 --runInBand ",
    "test:cov": "jest --forceExit --coverage  --maxConcurrency=8 --runInBand ",
    "cov:show": "./coverage/lcov-report/index.html",
    "torm": "ts-node --swc -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "yarn torm migration:create src/migrations/DropPrintCompletion",
    "typeorm:generate": "yarn torm migration:generate -d src/data-source.ts --pretty src/migrations/AddPrintJob",
    "typeorm:migrate": "yarn torm migration:run -d src/data-source.ts",
    "typeorm:revert": "yarn torm migration:revert -d src/data-source.ts"
  },
  "keywords": [
    "fdm-monster",
    "fdm-connector",
    "octoprint",
    "moonraker",
    "klipper",
    "bambu",
    "prusalink",
    "fdm",
    "3d printing",
    "pi",
    "node",
    "vue"
  ],
  "resolutions": {
    "tr46": "6.0.0"
  },
  "dependencies": {
    "@fdm-monster/client-next": "2.2.3",
    "@octokit/plugin-throttling": "8.2.0",
    "@sentry/node": "10.33.0",
    "adm-zip": "0.5.16",
    "awilix": "12.0.5",
    "awilix-express": "9.0.2",
    "axios": "1.13.2",
    "basic-ftp": "5.1.0",
    "bcryptjs": "3.0.3",
    "better-sqlite3": "12.6.0",
    "class-validator": "0.14.3",
    "connect-history-api-fallback": "2.0.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "eventemitter2": "6.4.9",
    "express": "4.22.1",
    "form-data": "4.0.5",
    "helmet": "8.1.0",
    "js-yaml": "4.1.1",
    "jsonwebtoken": "9.0.3",
    "lodash": "4.17.21",
    "luxon": "3.7.2",
    "mqtt": "5.14.1",
    "multer": "2.0.2",
    "octokit": "3.2.2",
    "passport": "0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "prom-client": "15.1.3",
    "reflect-metadata": "0.2.2",
    "semver": "7.7.3",
    "socket.io": "4.8.3",
    "toad-scheduler": "3.1.0",
    "typeorm": "0.3.28",
    "uuid": "11.1.0",
    "winston": "3.19.0",
    "winston-loki": "6.1.3",
    "ws": "8.19.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@lcov-viewer/cli": "1.3.0",
    "@lcov-viewer/istanbul-report": "1.4.0",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.8",
    "@swc/jest": "0.2.39",
    "@types/adm-zip": "0.5.7",
    "@types/connect-history-api-fallback": "1.5.4",
    "@types/cookie-parser": "1.4.10",
    "@types/express": "5.0.1",
    "@types/ip": "1.1.3",
    "@types/jest": "29.5.14",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.17.23",
    "@types/luxon": "3.7.1",
    "@types/multer": "2.0.0",
    "@types/node": "24.10.7",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "4.0.1",
    "@types/semver": "7.7.1",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.18.1",
    "chokidar": "5.0.0",
    "ftp-srv": "4.6.3",
    "jest": "29.7.0",
    "jest-27-expect-message": "1.1.0",
    "nock": "13.5.6",
    "openapi3-ts": "4.5.0",
    "selfsigned": "5.4.0",
    "supertest": "7.2.2",
    "swagger-ui-dist": "5.31.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3"
  },
  "ignore-engines": false,
  "engine-strict": true,
  "engines": {
    "node": ">= 22.0.0",
    "npm": ">= 10.9.0",
    "yarn": ">= 4.12.0"
  },
  "packageManager": "yarn@4.12.0"
}
